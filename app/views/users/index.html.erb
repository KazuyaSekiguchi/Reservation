 <h3>ユーザー情報一覧</h3>
 
 <table>
   <thead>
     <tr>
       <th>id</th>
       <th>アイコン</th>
       <th>名前</th>
       <th>メールアドレス</th>
       <th>パスワード</th>
       <th>更新日</th>
       <th>作成日</th>
       <th>参照</th>
       <th>編集</th>
     </tr>
   </thead>
 
   <tbody>
     <% @users.each do |user| %>
       <tr>
         <td><%= user.id %></td>
         <td><%= user.User_icon %></td>
         <td><%= user.name %></td>
         <td><%= user.email %></td>
         <td>********</td>
         <td><%= user.updated_at.to_s(:datetime_jp) %></td>
         <td><%= user.created_at.to_s(:datetime_jp) %></td>
         <td>
         <%= link_to "参照", user %>
         </td>
         <td>
         <%= link_to "編集", [:edit, user] %>
         </td>
       </tr>
     <% end %>
     
      <h3>投稿一覧</h3>
 
 <table>
   <thead>
     <tr>
       <th>投稿内容</th>
       <th>更新日時</th>
       <th>投稿日時</th>
       <th>投稿者(User_id)</th>
     </tr>
   </thead>
 
   <tbody>
     <% @posts.each do |post| %>
       <tr>
         <td><%= post.content %></td>
         <td><%= post.updated_at.to_s(:datetime_jp) %></td>
         <td><%= post.created_at.to_s(:datetime_jp) %></td>
         <td><%= post.user_id %></td>
       </tr>
     <% end %>
   </tbody>
 </table>
 